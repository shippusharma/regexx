{
	"name": "regexx",
	"version": "1.1.2",
	"description": "regexx, a versatile and efficient regex library designed to streamline pattern matching and text manipulation tasks in your programming projects. This comprehensive library provides developers with a rich set of pre-built regular expressions for common use cases, including email and URL validation, password strength checks, and more.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"engines": {
		"node": ">=16"
	},
	"keywords": [
		"regex",
		"regex-js",
		"regular",
		"expression",
		"regular-expression",
		"regex-pattern",
		"javascript",
		"typescript",
		"node",
		"regexx"
	],
	"author": {
		"name": "Shippu Sharma",
		"email": "shippusharma00@gmail.com",
		"url": "https://github.com/shippusharma"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shippusharma/regexx.git"
	},
	"homepage": "https://github.com/shippusharma/regexx#readme",
	"bugs": {
		"url": "https://github.com/shippusharma/regexx/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "nodemon --exec ts-node src/index.ts",
		"build": "npm run clean && tsc",
		"start": "node lib/index.js",
		"clean": "rm -rf lib tsconfig.tsbuildinfo",
		"prepare": "npm run build",
		"test": "jest tests/index.test.ts --verbose",
		"test:jest": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:update": "jest --updateSnapshot"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^22.9.1",
		"jest": "^29.7.0",
		"nodemon": "^3.0.2",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3"
	}
}
